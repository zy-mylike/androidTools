<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    tools:context="com.enetic.push.activity.DataanalysisActivity">

    <include layout="@layout/layout_activity_header_main" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">



            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/px_3"
                android:layout_marginBottom="@dimen/px_4"
                android:layout_marginLeft="@dimen/px_6"
                android:textColor="#FF3D3D3D"
                android:text="销售额" />

            <lecho.lib.hellocharts.view.ColumnChartView
                android:id="@+id/chart1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:paddingRight="@dimen/px_16"
                android:paddingLeft="@dimen/px_16"
         />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/px_3"
            android:layout_marginBottom="@dimen/px_4"
            android:layout_marginLeft="@dimen/px_6"
            android:textColor="#FF3D3D3D"
            android:text="销售额" />

            <lecho.lib.hellocharts.view.LineChartView
                android:id="@+id/chart2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:paddingRight="@dimen/px_16"
                android:paddingLeft="@dimen/px_16"
                 />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/px_3"
            android:layout_marginBottom="@dimen/px_4"
            android:layout_marginLeft="@dimen/px_6"
            android:textColor="#FF3D3D3D"
            android:text="销售额" />

            <lecho.lib.hellocharts.view.ColumnChartView
                android:id="@+id/chart3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:paddingRight="@dimen/px_16"
                android:paddingLeft="@dimen/px_16"
              />
        </LinearLayout>

</LinearLayout>